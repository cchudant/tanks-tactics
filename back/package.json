    "@types/passport-local": "^1.0.34",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
