{
  "name": "back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js -d ormconfig.ts",
    "db:schema:sync": "yarn typeorm schema:sync",
    "db:schema:log": "yarn typeorm schema:log",
    "db:schema:drop": "yarn typeorm schema:drop",
    "db:migration:create": "yarn typeorm migration:create",
    "db:migration:generate": "yarn typeorm migration:generate",
    "db:migration:run": "yarn typeorm migration:run",
    "db:migration:show": "yarn typeorm migration:show",
    "db:migration:revert": "yarn typeorm migration:revert",
    "typeorm:prod": "ts-node --transpile-only ./node_modules/typeorm/cli.js -d ./dist/ormconfig.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.1",
    "@nestjs/core": "^9.0.1",
    "@nestjs/event-emitter": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.1",
    "@nestjs/typeorm": "^9.0.0",
    "@nestjs/websockets": "^9.0.1",
    "argon2": "^0.28.7",
    "bull": "^3.29.1",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "dotenv": "^10.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "socket.io": "^4.2.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/argon2": "^0.15.0",
    "@types/bull": "^3.15.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.10",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/socket.io": "^3.0.2",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  }
}
